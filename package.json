{
    "name": "astar_demo",
    "version": "1.0.0",
    "description": "demo of the A* pathfinding algorithm",
    "private": true,
    "mode": "development",
    "scripts": {
        "prestart": "npm run build",
        "start": "node ./scripts/open.js",
        "poststart": "npm run watch",
        "prestart:noformat": "npm run build:noformat",
        "start:noformat": "node ./scripts/open.js",
        "poststart:noformat": "npm run watch",
        "build": "npm run format && npm run build:clean && npm run build:js",
        "build:noformat": "npm run build:clean && npm run build:js",
        "build:clean": "node ./scripts/clean.js",
        "build:js": "webpack",
        "watch": "webpack --watch",
        "format": "npm run prettier:fix && npm run lint:fix",
        "lint:check": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "prettier:fix": "prettier --write src/**/*.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@types/p5": "^1.4.1",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "@webpack-cli/generators": "^1.1.0",
        "babel-loader": "^8.2.3",
        "eslint": "^8.10.0",
        "path-browserify": "^1.0.1",
        "prettier": "^2.5.1",
        "shelljs": "^0.8.5",
        "source-map-loader": "^3.0.1",
        "typescript": "^4.6.2",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "p5": "^1.4.1"
    }
}
